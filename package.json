{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "simple CRUD API",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec ts-node-dev src/index.ts",
    "start:prod": "npm run build && node dist/index.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
    
  },
  "author": "Balashova Maryna",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@types/uuid": "^9.0.2",
    "eslint": "^8.44.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BalashovaMa/CrudAPI"
  },
  "dependencies": {
    "nodemon": "^2.0.22"
  }
}
